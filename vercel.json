{
  "buildCommand": "chmod +x build.sh && bash build.sh",
  "installCommand": "echo 'Skipping install, using buildCommand'",
  "rewrites": [
    {
      "source": "/((?!static).*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "PYTHONDONTWRITEBYTECODE": "1"
  }
}

